# LLM via ProxyAPI (example). Do NOT commit your real key.
LLM_ENABLED=0
LLM_PROVIDER=openai
LLM_BASE_URL=https://api.proxyapi.ru/openai/v1
LLM_MODEL=gpt-4o-mini
LLM_API_KEY=__PUT_YOUR_KEY_HERE__
LLM_TIMEOUT_S=30

# --- Inpainting (Before/After) ---
# auto | sd | cv
INPAINT_METHOD=auto

# Enable Stable Diffusion inpainting (best quality). If 0 -> OpenCV fallback.
INPAINT_SD_ENABLED=0
INPAINT_SD_MODEL=runwayml/stable-diffusion-inpainting
INPAINT_SD_DEVICE=cuda
INPAINT_SD_DTYPE=fp16
INPAINT_SD_STEPS=25
INPAINT_SD_GUIDANCE=6.5
INPAINT_SD_STRENGTH=0.98
INPAINT_SD_NEGATIVE=damage, dent, scratch, crack, broken, rust, text, watermark, label, box, annotation
